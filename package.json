{
  "name": "@djrhails/watcher-ui",
  "version": "0.4.2",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "url": "git://github.com/DJRHails/watcher-ui.git"
  },
  "dependencies": {
    "@djrhails/watcher-icons": "1.0.0",
    "@ethersproject/address": "^5.0.1",
    "@mdx-js/react": "^1.6.16",
    "@popperjs/core": "^2.4.4",
    "@react-aria/interactions": "^3.1.0",
    "@styled-system/css": "^5.1.5",
    "aria-hidden": "^1.1.1",
    "core-js": "^3.6.5",
    "jazzicon": "^1.5.0",
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-remove-scroll": "^2.3.0",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "concurrently yarn:build:esm yarn:build:cjs yarn:build:types",
    "build:esm": "cross-env BABEL_ENV=esm babel src --extensions .ts,.tsx -d dist/esm --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --extensions .ts,.tsx -d dist/cjs --source-maps",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap --declarationDir dist/types && tscpaths -p tsconfig.json -s src -o dist/types",
    "generate": "plop --plopfile src/generators/plobfile.ts",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "storybook:deploy": "storybook-to-ghpages",
    "type": "tsc --noEmit",
    "type:watch": "npm run type -- --watch",
    "test": "yarn jest",
    "lint": "concurrently yarn:lint:ts yarn:lint:js",
    "lint:fix": "concurrently yarn:lint:ts:fix yarn:lint:js:fix",
    "lint:ts": "eslint \"src/**/*.ts?(x)\"",
    "lint:ts:fix": "eslint \"src/**/*.ts?(x)\" --fix",
    "lint:js": "prettier --ignore-path .gitignore --check \"**/*.{js,json}\"",
    "lint:js:fix": "prettier --ignore-path .gitignore --write \"**/*.{js,json}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@storybook/addon-a11y": "^6.0.13",
    "@storybook/addon-actions": "^6.0.13",
    "@storybook/addon-docs": "^6.0.13",
    "@storybook/addon-knobs": "^6.0.13",
    "@storybook/addon-links": "^6.0.13",
    "@storybook/addons": "^6.0.13",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.0.13",
    "@storybook/storybook-deployer": "^2.8.6",
    "@testing-library/dom": "^7.22.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^5.1.2",
    "@types/styled-system": "^5.1.10",
    "@types/styled-system__css": "^5.0.11",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-react-app": "^9.1.2",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-react": "^7.20.0",
    "jest": "^26.0.1",
    "plop": "^2.7.1",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "tscpaths": "^0.0.9",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5",
    "webpack": "^4.44.1"
  }
}
